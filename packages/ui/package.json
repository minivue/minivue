{
  "name": "@minivue/ui",
  "version": "0.1.7",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "minivue dev --lib",
    "build": "minivue build --lib",
    "check": "tsc --incremental --noEmit"
  },
  "files": [
    "api",
    "components",
    "miniprogram_dist",
    "dist",
    "utils",
    "index.d.ts",
    "shims-vue.d.ts",
    "type.ts"
  ],
  "miniprogram": "miniprogram_dist",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": "./dist/components"
  },
  "peerDependencies": {
    "@minivue/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@minivue/cli": "workspace:*",
    "vue": "latest",
    "typescript": "latest",
    "miniprogram-api-typings": "latest"
  },
  "packageManager": "pnpm@10.3.0",
  "engines": {
    "node": ">=20.15.1"
  }
}
