{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputLogs": "full"
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputLogs": "full"
    }
  }
}
