{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "check": {},
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "outputLogs": "full"
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "outputLogs": "full"
    },
    "dev:web": {
      "cache": false,
      "persistent": true,
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "outputLogs": "full"
    }
  }
}
